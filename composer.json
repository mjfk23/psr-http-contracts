{
  "name": "mjfk23/psr-http-contracts",
  "description": "Collection of PSR HTTP interfaces",
  "type": "library",
  "keywords": [
    "psr",
    "psr-7",
    "http",
    "http-message",
    "request",
    "response"
  ],
  "homepage": "https://github.com/mjfk23/psr-http-contracts",
  "license": "MIT",
  "authors": [
    {
      "name": "Melody Kimball",
      "role": "lead"
    }
  ],
  "support": {
    "issues": "https://github.com/mjfk23/psr-http-contracts/issues",
    "wiki": "https://github.com/mjfk23/psr-http-contracts/wiki",
    "source": "https://github.com/mjfk23/psr-http-contracts"
  },
  "require": {
    "php": "^8.1",
    "fig/http-message-util": "^1.1",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.1",
    "psr/http-message": "^2.0",
    "psr/http-server-middleware": "^1.0",
    "psr/http-server-handler": "^1.0"
  },
  "require-dev": {
    "mjfk23/gadget-code-style": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Fig\\Http\\Message\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "apcu-autoloader": true
  },
  "scripts": {
    "phpcs": "vendor/bin/phpcs",
    "phpstan": "vendor/bin/phpstan analyse -c phpstan.neon"
  }
}
